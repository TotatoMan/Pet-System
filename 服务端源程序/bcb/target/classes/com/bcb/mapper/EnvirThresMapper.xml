<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bcb.mapper.EnvirThresMapper">
    <update id="thresupdate">
     update envirthres
     set
        tempLow=#{tempLow},
        tempHigh=#{tempHigh},
        humidLow=#{humidLow},
        humidHigh=#{humidHigh}
        where id=1
    </update>
</mapper>